  "license": "WTFPL",
  "repository": {
    "type": "git",
    "url": "git://github.com/domenic/sinon-chai.git"
  },
  "bugs": {
    "url": "http://github.com/domenic/sinon-chai/issues"
  },
  "directories": {
    "lib": "./lib"
